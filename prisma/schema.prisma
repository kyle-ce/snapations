
generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Memes {
  id        String   @id @default(cuid())
  userId    String   // ‚Üê will store auth.uid()
  imageUrl  String
  caption   String
  createdAt DateTime @default(now())
}
